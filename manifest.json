{
    "manifest_version": 3,
    "author": "ddorstijn",
    "description": "wp",
    "name": "workpage",
    "homepage_url": "https://github.com/ddorstijn/workpage",
    "version": "0.5", 

    "icons": {
        "32": "icons/32.png",
        "48": "icons/48.png",
        "64": "icons/64.png"
    },
    "permissions": ["tabs", "storage"],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },

    "browser_specific_settings": {
        "gecko": {
            "id": "dannydorstijn1997@gmail.com"
        }
    },

    "action": {
        "default_icon": {
            "32": "icons/32.png"
        },
        "default_popup": "src/entries/popup/index.html"
    }
}