{
  "name": "workpage",
  "version": "1.0.0",
  "displayName": "Workpage",
  "author": "ddorstijn",
  "description": "Adjust your new tab page to your personal needs. Use projects to organise your links and tasks for a productive start of your tabs.",
  "homepage_url": "https://github.com/ddorstijn/workpage",
  "scripts": {
    "build": "tsc && vite build",
    "watch": "vite build --watch --mode development --minify false",
    "dev": "vite",
    "serve:firefox": "web-ext run --start-url \"about:debugging#/runtime/this-firefox\"  --source-dir ./dist/",
    "serve:chrome": "web-ext run -t chromium --start-url \"https://example.com\" --source-dir ./dist/"
  },
  "license": "MIT",
  "devDependencies": {
    "@iconify-json/ic": "^1.1.13",
    "@samrum/vite-plugin-web-extension": "^5.0.0",
    "@types/chrome": "^0.0.228",
    "@types/webextension-polyfill": "^0.10.0",
    "@unocss/preset-icons": "^0.53.1",
    "solid-dnd-directive": "^0.2.0",
    "typescript": "^5.1.3",
    "unocss": "^0.53.1",
    "vite": "~4.3.3",
    "vite-plugin-solid": "^2.7.0",
    "web-ext": "^7.6.2"
  },
  "dependencies": {
    "@solid-primitives/date": "^2.0.14",
    "@unocss/reset": "^0.53.1",
    "@zag-js/combobox": "^0.10.2",
    "@zag-js/dialog": "^0.10.2",
    "@zag-js/solid": "^0.10.2",
    "@zag-js/tooltip": "^0.10.2",
    "solid-js": "^1.7.6",
    "webextension-polyfill": "^0.10.0"
  }
}
