{
	"name": "My Workpage",
	"description": "Adjust your new tab page to your personal needs. Use projects to organise your links and tasks for a productive start of your tabs.",
	"homepage_url": "https://github.com/ddorstijn/workpage",
	"manifest_version": 2,
	"version": "1.3.3",
	"browser_specific_settings": {
		"gecko": {
			"id": "dannydorstijn1997@gmail.com"
		}
	},
	"chrome_url_overrides" : {
		"newtab": "newtab/index.html"
	},
  "browser_action": {
    "default_popup": "popup/index.html",
    "default_icon": {
			"32": "logo/logo-32.png",
			"48": "logo/logo-48.png",
			"64": "logo/logo-64.png"
    }
  },
	"icons": {
		"32": "logo/logo-32.png",
		"48": "logo/logo-48.png",
		"64": "logo/logo-64.png"
	},
	"permissions": ["storage", "tabs"]
}